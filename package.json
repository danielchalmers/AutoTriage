{
  "name": "autotriage",
  "version": "0.1.0",
  "description": "AI triage for GitHub issues & PRs: summarizes, labels, comments, retitles and can close items using a project prompt + Gemini.",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "build": "npm run typecheck && ncc build src/index.ts -o dist -s --minify --license licenses.txt",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "dev": "tsc -w",
    "prepare": "npm run build"
  },
  "keywords": [
    "github-actions",
    "triage",
    "issues",
    "pull-requests",
    "ai",
    "gemini"
  ],
  "author": "Daniel Chalmers",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist",
    "action.yml",
    "README.md"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@vercel/ncc": "^0.38.1",
    "typescript": "^5.5.0",
  "rimraf": "^5.0.0",
  "@types/node": "^20.11.0"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^6.0.0"
  }
}
